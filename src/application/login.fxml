<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <TextField fx:id="username" layoutX="106.0" layoutY="45.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="106.0" layoutY="88.0" promptText="password" />
      <Button fx:id="signup" layoutX="106.0" layoutY="145.0" mnemonicParsing="false" onAction="#Signup" text="sign up" />
      <Button fx:id="login" layoutX="206.0" layoutY="145.0" mnemonicParsing="false" onAction="#Login" text="Log in" />
      <Label fx:id="message" alignment="CENTER" layoutX="12.0" layoutY="14.0" prefHeight="17.0" prefWidth="336.0" textFill="RED" />
   </children>
</AnchorPane>
